using System;
class MaleFemale
{
    static void Main()
    {
        if (Console.ReadKey().Key == ConsoleKey.F)
        {
                Console.WriteLine("Your gender is female");
        }
        if (Console.ReadKey().Key == ConsoleKey.M)
        {
            Console.WriteLine("Your gender is male");
        }
        else
        {
            Console.WriteLine("error");
        }
    }
}



