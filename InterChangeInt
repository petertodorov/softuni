using System;
class InterChangeInt
{
    static void Main()
    {
        Console.WriteLine("write value of a:");
        int a = int.Parse(Console.ReadLine());
        Console.WriteLine("write value of b:");
        int b = int.Parse(Console.ReadLine());
        Console.WriteLine("Before: \n a = {0}; \n and \n b = {1};", a, b);
        int c = a;
        int aCh = b;
        int bCh = c;
        Console.WriteLine("Now: \n a = {0}; \n and \n b = {1};", aCh, bCh);
    }
}
