using System;
class NullValuesArithmetic
{
    static void Main()
    {
        int? nullable = null;
        double? nullableDouble = null;
        int n = int.Parse(Console.ReadLine());
        Console.WriteLine("The vallue of nullable is:{0} and the value of double nullable is:{1}"
            ,nullable, nullableDouble);
        Console.WriteLine(nullable +n);
        Console.WriteLine(nullableDouble + n+1);
    }
}

