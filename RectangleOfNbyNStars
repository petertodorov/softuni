using System;
class RectangleOfNbyNStars
{
    static void Main()
    {
        int n = int.Parse(Console.ReadLine());
        for (int row = 0; row < n; row++)
        {
            for (int col = 0; col < n; col++)
            {
                Console.Write("*");
            }
            Console.WriteLine();
        }
    }
}
