using System;
class Factorial
{
    static void Main()
    {
        int a = int.Parse(Console.ReadLine());
        while (a >= 1 && a <= 12)
        {
            int factorial = 1;
            for (int i = 2; i <= a; i++)
            {
                factorial *= i;
            }
            Console.WriteLine(factorial);
            break;
        }
    }
}
