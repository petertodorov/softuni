using System;
class ThirdFigitIsSeven
{
    static void Main()
    {
        while (true)
        {
            Console.WriteLine("enter number:");
            int number = int.Parse(Console.ReadLine());
            int thirdDigit = number / 100;
            int digitFind = thirdDigit % 10;
            bool check = digitFind == 7;
            Console.WriteLine(check);
        }
    }
}
